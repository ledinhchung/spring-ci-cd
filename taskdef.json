{
  "executionRoleArn": "arn:aws:iam::110008036506:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "spring-website",
      "image": "110008036506.dkr.ecr.us-east-1.amazonaws.com/spring-ci-cd",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048",
  "family": "spring"
}
